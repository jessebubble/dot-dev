{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "icons": "npx @svgr/cli --out-dir app/components/icons -- app/icons",
    "icons:watch": "npm-watch icons",
    "dev:svg": "run-s icons icons:watch",
    "dev:remix": "remix dev",
    "dev": "run-p dev:*",
    "build": "remix build",
    "typecheck": "tsc"
},
"watch": {
    "icons": {
        "patterns": [
            "icons"
        ],
        "extensions": "svg",
        "quiet": false
    }
},
  "dependencies": {
